<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="anim_choose_up_from_bottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/h_fragment_enter</item>
        <item name="android:windowExitAnimation">@anim/h_fragment_exit</item>
    </style>
    <style name="personal_tool_bar_expanded" parent="Base.TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="personal_tool_bar_collapsed" parent="Base.TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>